<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="om_crud_Opration.Games" table="Games_2011">
		<id name="gamesId" column="games_Id">
		</id>
		<property name="gamesName" column="games_Name" />
		<property name="gamesTotalplayer" column="games_Totalplayer" />
		<many-to-one name="parentObjects" column="Ydummy"
			class="om_crud_Opration.Playground" cascade="all" />
	</class>
</hibernate-mapping>
